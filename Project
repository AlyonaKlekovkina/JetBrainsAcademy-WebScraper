import requests

print('Input the URL:')
url = input()
r = requests.get(url, headers={'Accept': 'application/json'})
file = open('source.html', 'w', encoding='utf-8')
if r.status_code == 200:
    file.write(r.text)
    print('Content saved.')

else:
    print('The URL returned {}!'.format(r.status_code))
